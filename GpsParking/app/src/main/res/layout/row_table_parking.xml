<?xml version="1.0" encoding="utf-8"?>



<TableRow xmlns:android="http://schemas.android.com/apk/res/android"

    android:background="@android:color/background_light"
    android:padding="5dp"
    android:id="@+id/row_table_parking"
    >
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:id="@+id/id_parking"
        ></TextView>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:id="@+id/designation_paking"
        ></TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:id="@+id/proprietaire_parking"
        ></TextView>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:id="@+id/date_ajout_parking"
        ></TextView>


    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_weight="1"
        android:baselineAlignBottom="false"
        android:contentDescription="Supprimer"
        android:src="@drawable/gps"
        android:id="@+id/btn_localisation"
        ></ImageButton>


</TableRow>